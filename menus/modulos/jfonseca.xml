		</menu>
	</menu>		
	
	<!--Módulo: TESTES (TST)-->
	<menu id="mTST" imageMso="AppointmentColorDialog" label="Testes">
		<menu id="smTST-Cadastros" image="cadastros" label="Cadastros">
			<button id="smTST-Cadastros-Contatos" imageMso="AppointmentColorDialog" label="Contatos" onAction="naAcaoBotao" tag="fContatos"/>
		</menu>
	</menu>	
</menu>
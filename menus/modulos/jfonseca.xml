<menu xmlns="http://schemas.microsoft.com/office/2006/01/customui" itemSize= "large">
	<!--
		SIGLAS
		m: Menu
		sm: Submenu
		CCI: Construção Civil
		FPE: Finanças Pessoais
		MOC: Medicina Ocupacional
	-->
	<!--Módulo: CONSTRUÇÃO CIVIL (CCI)-->
	<menu id="mCCI" image="CCI" label="Construção Civil">
		<menu id="smCCI-Cadastros" image="cadastros" label="Cadastros">
			<button id="smCCI-Cadastros-Clientes" imageMso="AppointmentColorDialog" label="Clientes" onAction="naAcaoBotao" tag="fClientes"/>
			<button id="smCCI-Cadastros-Fornecedores" imageMso="AppointmentColorDialog" label="Fornecedores" onAction="naAcaoBotao" tag="fFornecedores"/>
			<button id="smCCI-Cadastros-UMs" imageMso="AppointmentColorDialog" label="Unidades de Medida" onAction="naAcaoBotao" tag="fUMs"/>
		</menu>
	</menu>			
	<!--Módulo: FINANÇAS PESSOAIS (FPE)-->
	<menu id="mFPE" image="FPE" label="Finanças Pessoais">
		<menu id="smFPE-Cadastros" image="cadastros" label="Cadastros">
			<button id="smFPE-Cadastros-Contas" imageMso="AppointmentColorDialog" label="Contas" onAction="naAcaoBotao" tag="fContas"/>
			<button id="smFPE-Cadastros-Categorias" imageMso="AppointmentColorDialog" label="Categorias" onAction="naAcaoBotao" tag="fCategorias"/>
			<button id="smFPE-Cadastros-Lojas" imageMso="AppointmentColorDialog" label="Lojas" onAction="naAcaoBotao" tag="fLojas"/>
